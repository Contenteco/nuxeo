      "alias(secondYAxis(servers.octopussy.loadavg.processes_running),\"process running\")",
      "alias(servers.octopussy.loadavg.15,\"15min\")",
      "alias(servers.octopussy.loadavg.05,\"5min\")",
      "alias(servers.octopussy.loadavg.01,\"1min\")"
    ]
