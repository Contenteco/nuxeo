## DO NOT EDIT THIS FILE, USE nuxeo.conf  ##

common-deploydir.target=server/default/deploy

# Authentication
mailservice.user=nobody
mailservice.password=password
# Mail server protocol
mail.store.protocol=pop3
mail.transport.protocol=smtp
# User who will receive mail
mail.user=nobody
# Mail server
mail.pop3.host=pop3.nosuchhost.nosuchdomain.com
# SMTP gateway server
mail.smtp.host=smtp.nosuchhost.nosuchdomain.com
# Mail server port
mail.smtp.port=25
# The address mail will be from
mail.from=nobody@nosuchhost.nosuchdomain.com
# Enable debugging output from the javamail classes
mail.debug=false
