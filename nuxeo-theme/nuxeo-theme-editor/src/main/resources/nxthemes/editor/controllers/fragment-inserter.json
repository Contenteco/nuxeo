  {"id": "fragment inserter",
   "type": "drag-and-drop",
   "dragging": {
       "element": "nxthemesFragmentFactory",
       "offset_x": -5,
       "offset_y": -5,
       "feedback": {
           "opacity": 0.8,
           "clone": false,
           "background": "#fc0",
           "border": "#000"
        },
        "zoomback": {
            "duration": 600
        },
        "opacity": 1
    },
    "dropping": {
        "target": "nxthemesContainer",
        "action": "insert fragment"
     }
  }
