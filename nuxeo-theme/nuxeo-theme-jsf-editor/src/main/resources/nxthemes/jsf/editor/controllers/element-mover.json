  {"id": "element mover",
   "type": "drag-and-drop",
   "dragging": {
     "source": "nxthemesDraggable",
     "highlight": {
       "color": "#fce94f"
     },
     "feedback": {
       "opacity": 0.25,
       "clone": false
     },
     "cursor": "default"
   },
   "shifting": {
     "element": "nxthemesDraggable",
     "container": "nxthemesContainer"
   },
   "dropping": {
     "action": "move element",
     "zoomto": {
       "duration": 200
     }
   }
 }
